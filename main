#include<stdio.h>
#include"myMath.h"


int main(){
    printf("please insert a real number:");
     double x;
     scanf("%lf",&x);
     double F1=sub(add(Exponent((int)x),Power(x,3)),2);
     double F2=add(mul(x,3),mul(2,Power(x,2)));
     double  F3=sub(div(mul(Power(x,3),4),5),mul(x,2));
 
printf("The value of f(x)=e^x+x^3-2 at the point %lf is:%.4lf\n",x,F1 );
printf("The value of f(x)=3x+2x^2 at the point %lf is:%.4lf\n",x,F2 );
printf("The value of f(x)=(4x^3)/5-2x at the point %lf is:%.4lf\n",x,F3 );
return 0;

}
